{
  "openapi": "3.0.0",
  "info": {
    "title": "EcoFit Rest API",
    "version": "1.0.0",
    "description": "SW 중심대학 통합 해커톤 EcoFit 프로젝트 Rest API Dcoument"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "challenge/": {
      "get": {
        "tags": [
          "challenge"
        ],
        "description": "challenge 임시",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/habit/": {
      "get": {
        "tags": [
          "Habit"
        ],
        "description": "습관을 조회하는 기초 API, 테스트용도",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/habit/add/{title}": {
      "put": {
        "tags": [
          "Habit"
        ],
        "description": "습관을 추가해주는 API입니다. 매개변수 title을 입력하여, 습관을 추가할수 있습니다.",
        "parameters": [
          {
            "name": "title",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "습관을 추가하기 위해 필요한 PK"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/habit/click/{title}": {
      "put": {
        "tags": [
          "Habit"
        ],
        "description": "습관을 클릭하면, count를 올려주는 API, 클릭할때마다 1씩 증가",
        "parameters": [
          {
            "name": "title",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "습관을 검색하기 위해 필요한 PK"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/market/": {
      "get": {
        "tags": [
          "Market"
        ],
        "description": "마켓의 상품들을 검색하는 API",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/market/data/image": {
      "get": {
        "description": "",
        "parameters": [],
        "responses": {}
      }
    },
    "/market/add/{name}/{price}/{explanation}/{hashtag}": {
      "post": {
        "tags": [
          "Market"
        ],
        "description": "",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "습관을 검색하기 위해 필요한 PK"
          },
          {
            "name": "price",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "explanation",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "hashtag",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/user/": {
      "get": {
        "tags": [
          "User"
        ],
        "description": "임시",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
}